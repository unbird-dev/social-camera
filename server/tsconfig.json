  "compilerOptions": {
    "allowJs": false,
    "downlevelIteration": true,
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
